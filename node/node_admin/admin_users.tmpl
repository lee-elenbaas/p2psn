<% c++ #include "../node_admin/admin_content.h" %>
<% skin ptp2n_node_admin_skin %>
<% view admin_users uses content::admin_users extends master %>

<% template page_content() %>
<table>
	<thead>
	<tr>
		<th>Name</th>
		<th>Password</th>
	</tr>
	</thead>
<% foreach user in users %>
	<tbody>
<% item %>
<tr>
	<td><%= user.name %></td>
	<td><%= user.password %></td>
</tr>
<% end %>
</tbody>
<% end %>
<% end template %> 

<% end view %>
<% end skin %>

